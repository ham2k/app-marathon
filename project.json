{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/marathon/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/marathon",
        "index": "apps/marathon/src/index.html",
        "baseHref": "/",
        "main": "apps/marathon/src/main.js",
        "polyfills": "apps/marathon/src/polyfills.js",
        "tsConfig": "apps/marathon/tsconfig.app.json",
        "assets": ["apps/marathon/src/favicon.ico", "apps/marathon/src/assets", "apps/marathon/src/netlify.toml"],
        "styles": ["apps/marathon/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "apps/marathon/webpackConfig.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/marathon/src/environments/environment.js",
              "with": "apps/marathon/src/environments/environment.prod.js"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "options": {
        "buildTarget": "marathon:build",
        "hmr": true,
        "port": 4202
      },
      "configurations": {
        "production": {
          "buildTarget": "marathon:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "marathon:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/marathon/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/marathon"],
      "options": {
        "jestConfig": "apps/marathon/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
